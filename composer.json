{
  "name": "grrr-amsterdam/nova-publish",
  "description": "Adds website publish button to Nova.",
  "keywords": [
    "laravel",
    "nova"
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://nova.laravel.com"
    }
  ],
  "license": "MIT",
  "require": {
    "php": "^8.0|^8.1|^8.2",
    "guzzlehttp/guzzle": "^7.3",
    "laravel/nova": "^4.0"
  },
  "autoload": {
    "psr-4": {
      "Publish\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Publish\\ToolServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "orchestra/testbench": "^6.0",
    "phpunit/phpunit": "^9.5"
  }
}
