{
  "name": "grrr-amsterdam/nova-publish",
  "description": "Adds website publish button to Nova.",
  "keywords": [
    "laravel",
    "nova"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.1.0"
  },
  "autoload": {
    "psr-4": {
      "Publish\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Nova\\Publish\\ToolServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
